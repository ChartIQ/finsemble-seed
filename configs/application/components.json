                "top": "center",
                "height": 800,
                "width": 1000
            },
            "component": {},
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
