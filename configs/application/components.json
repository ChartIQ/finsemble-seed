                "category": "system"
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": false
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": false
                    },
                    "Window Manager": {
                        "FSBLHeader": false,
                        "persistWindowState": false
                    }
                }
            }
