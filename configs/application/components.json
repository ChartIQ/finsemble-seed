        "Advanced Chart": {
            "window": {
                "url": "$applicationRoot/components/chartiq/chart/sample-template-advanced.html",
                "width": 800,
                "height": 600,
                "options": {
                    "minWidth": 75
                }
            },
            "component": {
                "mode": "premium",
                "advertiseReceivers": [
                    "chartiq.chart",
                    "symbol"
                ],
                "spawnOnHotkey": [
                    "ctrl",
                    "alt",
                    "a"
                ],
                "category": "system",
                "preload": "$applicationRoot/components/chartiq/chartiq.js"
