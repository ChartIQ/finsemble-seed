            "component": {
                "category": "system"
            },
            "window": {
                "frame": false,
                "alwaysOnTop": false,
                "url": "$applicationRoot/components/composites-menu/index.html",
                "width": 300,
                "height": 400,
                "ephemeral": true,
                "options": {
                    "autoShow": false
                }
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": false,
                        "shouldRegister": false
                    }
                },
                "components": {
                    "Window Manager": {
                        "persistWindowState": false,
                        "FSBLHeader": false,
                        "showTabs": false
                    }
                }
            }
        },
        "Composites Designer": {
            "window": {
                "url": "$applicationRoot/components/composites-designer/index.html",
