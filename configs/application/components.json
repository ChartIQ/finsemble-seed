{
    "comment": "Houses config for custom components that you'd like to import into Finsemble.",
    "components": {
        "Getting Started Tutorial": {
            "window": {
                "url": "https://documentation.chartiq.com/finsemble/tutorial-gettingStarted.html",
                "height": 800,
                "width": 1000
            },
            "component": {
                "spawnOnStartup": true
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "persistWindowState": true,
                        "FSBLHeader": true,
                        "showLinker": false
                    }
                }
            }
        },
        "Docking Move Mask": {
			"window": {
				"url": "$applicationRoot/components/src/dockingGroupMask/dockingGroupMask.html",
				"frame": false,
				"resizable": false,
				"showTaskbarIcon": false,
				"options": {
					"opacity": 0.7,
					"autoShow": false
				}
			},
			"component": {
				"category": "system"
			},
			"foreign": {
				"services": {
					"dockingService": {
						"isArrangable": false
					}
				},
				"components": {
					"App Launcher": {
						"launchableByUser": false
					},
					"Window Manager": {
						"FSBLHeader": false,
						"persistWindowState": false
					}
				}
			}
		}
    }
}