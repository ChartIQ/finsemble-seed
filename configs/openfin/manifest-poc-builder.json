{
    "devtools_port": 9090,
    "startup_app": {
        "name": "Finsemble POC Builder",
        "url": "https://fpe.finsemble.com/finsemble/components/system/serviceManager/serviceManager.html",
        "uuid": "Finsemble",
        "applicationIcon": "https://fpe.finsemble.com/components/assets/img/Finsemble_Taskbar_Icon.png",
        "defaultTop": 0,
        "defaultLeft": 0,
        "showTaskbarIcon": true,
        "autoShow": false,
        "frame": false,
        "resizable": false,
        "maximizable": false,
        "delay_connection": true,
        "contextMenu": true,
        "cornerRounding": {
            "width": 4,
            "height": 4
        },
        "alwaysOnTop": false
    },
    "runtime": {
        "arguments": "--noerrdialogs  --v=4 --no-sandbox  --enable-crash-reporting --framestrategy=frames",
        "version": "8.56.28.36"
    },
    "shortcut": {
        "company": "ChartIQ",
        "description": "Application used to bring in component configurations to build a Finsemble proof-of-concept.",
        "icon": "https://fpe.finsemble.com/components/assets/img/Finsemble_Taskbar_Icon.ico",
        "name": "Finsemble proof-of-concept builder",
        "target": [
            "desktop",
            "start-menu"
        ],
        "force": false,
        "startMenuRootFolder": "Finsemble"
    },
    "dialogSettings": {
        "logo": "https://fpe.finsemble.com/components/assets/img/ciq-banner-100x25.png",
        "bgColor": 4280109424,
        "textColor": 4293521652,
        "progressBarBgColor": 4294967295,
        "progressBarFillColor": 4282684881,
        "progressBarBorderColor": 4293521652
    },
    "supportInformation": {
        "company": "ChartIQ",
        "product": "Finsemble",
        "email": "support@finsemble.com"
    },
    "fileName": "FinsemblePOCBuilder-installer",
    "appAssets": [
        {
            "src": "http://connect2.chartiq.com/finsemble/Assimilation_1_9_5.zip",
            "version": "1.9.5",
            "alias": "assimilation",
            "target": "assimilationMain.exe"
        }
    ],
    "finsemble": {
        "applicationRoot": "https://fpe.finsemble.com",
        "moduleRoot": "https://fpe.finsemble.com/finsemble",
        "servicesRoot": "https://fpe.finsemble.com/finsemble/services",
        "notificationURL": "https://fpe.finsemble.com/components/notification/notification.html",
        "importConfig": [
            "$applicationRoot/configs/application/config.json"
        ]
    }
}