{
    "devtools_port": 9090,
    "startup_app": {
        "name": "ChartIQ Local",
        "url": "http://localhost:3375/components/system/serviceManager/serviceManager.html",
        "uuid": "Finsemble",
        "applicationIcon": "http://localhost:3375/components/samples/src/assets/img/Finsemble_Taskbar_Icon.png",
        "defaultTop": 0,
        "defaultLeft": 0,
        "showTaskbarIcon": true,
        "autoShow": false,
        "frame": false,
        "resizable": false,
        "maximizable": false,
        "delay_connection": true,
        "contextMenu": true,
        "cornerRounding": {
            "width": 4,
            "height": 4
        },
        "alwaysOnTop": false
    },
    "splashScreenImage": "https://finsemble-cdn.chartiq.com/sales-demo/components/assets/img/FinsembleSplash.png",
    "runtime": {
        "arguments": "--noerrdialogs  --v=1 --no-sandbox  --enable-crash-reporting",
        "version": "8.56.24.41"
    },
    "shortcut": {
        "company": "ChartIQ",
        "description": "ChartIQ Local",
        "icon": "http://localhost:3375/components/samples/src/assets/img/Finsemble_Taskbar_Icon.ico",
        "name": "ChartIQ - LOCAL",
        "target": [
            "desktop",
            "start-menu"
        ],
        "force": false,
        "startMenuRootFolder": "ChartIQ Local"
    },
    "dialogSettings": {
        "logo": "http://localhost:3375/components/samples/src/assets/img/ciq-banner-100x25.png",
        "bgColor": 4280109424,
        "textColor": 4293521652,
        "progressBarBgColor": 4294967295,
        "progressBarFillColor": 4282684881,
        "progressBarBorderColor": 4293521652
    },
    "supportInformation": {
        "company": "ChartIQ",
        "product": "Finsemble Local Demo",
        "email": "finsemble@chartiq.com"
    },
    "fileName": "ChartIQ-local-installer",
    "appAssets": [
        {
            "src": "http://connect2.chartiq.com/finsemble/Assimilation.zip",
            "version": "1.0.5",
            "alias": "assimilation",
            "target": "assimilationMain.exe"
        },
        {
            "src": "http://localhost:3375/hosted/putty.zip",
            "version": "1.0.0",
            "alias": "putty",
            "target": "putty.exe"
        }
    ],
    "finsemble": {
        "mode": [
            "dev",
            "premium",
            "djs"
        ],
        "applicationRoot": "http://localhost:3375/",
        "moduleRoot": "$applicationRoot",
        "forceRouterToOFB": false,
        "autoSignOn": {
            "hardcoded": {
                "rsrchx": {
                    "username": "terry@chartiq.com",
                    "password": "ireallylovecharts"
                },
                "symphony": {
                    "username": "Nomura",
                    "password": "Ilovecharts Ilovecharts1"
                }
            }
        },
        "importConfig": [
            "$applicationRoot/configs/application/config.json"
        ]
    }
}