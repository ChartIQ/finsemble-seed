{
  "components": {
    "Intent Resolver": {
      "window": {
        "url": "$applicationRoot/components/intentResolver/intentResolverModal.html",
        "resizable": false,
        "autoShow": true,
        "width": 420,
        "height": 420,
        "ephemeral": true,
        "slave": false,
        "left": "adjacent",
        "dockOnSpawn": true
      },
      "component": {
        "preload": "$applicationRoot/preloads/FDC3Client.js",
        "spawnOnStartup": true
      },
      "foreign": {
        "services": {
          "dockingService": {
            "canGroup": false,
            "isArrangable": false
          }
        },
        "clients": {
          "dialogManager": {
            "isDialog": true
          }
        },
        "components": {
          "App Launcher": {
            "launchableByUser": false
          },
          "Window Manager": {
            "showLinker": false,
            "FSBLHeader": false,
            "persistWindowState": false,
            "title": "Intent Resolver"
          },
          "Toolbar": {
            "iconClass": "ff-component"
          }
        }
      }
    }
  }
}