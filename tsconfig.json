{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2017",
		"typeRoots": ["node_modules/@types", "node_modules/@chartiq"]
	},
	"include": ["src/**/*"],
	"exclude": []
}
